<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>aws</groupId>
  <artifactId>aws-Lambda-Simple</artifactId>
  <name>AWS Lambda Simple App</name>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <finalName>hello-lambda-javaSimple</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.1.0</version>
        <configuration>
          <archive>
            <manifest>
              <mainClass>ocsb.mp.cloud.basic.JavaSimple</mainClass>
            </manifest>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <properties>
    <maven.compiler.target>11</maven.compiler.target>
    <aws.java.sdk.version>2.17.290</aws.java.sdk.version>
    <maven.compiler.source>11</maven.compiler.source>
    <maven.compiler.plugin.version>11</maven.compiler.plugin.version>
    <commons-io.version>2.11.0</commons-io.version>
  </properties>
</project>
